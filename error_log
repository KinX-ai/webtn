        SELECT 
            o.id,
            o.user_id,
            u.username,
            rt.name as resource_type,
            o.quantity,
            o.price,
            o.source,
            o.status,
            o.created_at,
            o.notification
        FROM orders o
        LEFT JOIN resource_types rt ON o.resource_type = rt.id
        LEFT JOIN users u ON o.user_id = u.id
        WHERE o.created_at BETWEEN :start_date AND :end_date_for_query ORDER BY o.created_at DESC
